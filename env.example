# Copy this file to .env and fill in your values

# Private key for deploying contracts (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URL for the network you want to deploy to
RPC_URL=https://ethereum-hoodi-rpc.publicnode.com

# Etherscan API key for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Discord username for Cadet role (if using Discord trap)
DISCORD_USERNAME=your_discord_username

# Operator addresses for trap configuration
OPERATOR_ADDRESS_1=0x...
OPERATOR_ADDRESS_2=0x...

# Response contract address (default for Hoodi testnet)
RESPONSE_CONTRACT=0x25E2CeF36020A736CF8a4D2cAdD2EBE3940F4608
